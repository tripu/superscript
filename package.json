{
  "name": "supers",
  "version": "0.1.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/tripu/superscript"
  },
  "description": "A library of miscellaneous JavaScript utilities",
  "license": "MIT",
  "main": "index",
  "bugs": {
    "url": "https://github.com/tripu/superscript/issues",
    "email": "t@tripu.info"
  },
  "private": false,
  "preferGlobal": "false",
  "devDependencies": {
    "browserify": "16.2.2",
    "coveralls": "3.0.1",
    "istanbul": "0.4.5",
    "mocha": "5.1.1",
    "nsp": "3.2.1"
  },
  "scripts": {
    "coverage": "istanbul cover _mocha",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls",
    "test": "mocha",
    "vulns": "nsp check",
    "build": "browserify index.js -s superscript -o dist/superscript.js"
  },
  "engines": {
    "node": "6 || 8 || 10",
    "npm": "5"
  }
}
