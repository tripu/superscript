{
  "name": "supers",
  "version": "0.1.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/tripu/superscript"
  },
  "homepage": "https://github.com/tripu/superscript",
  "description": "A library of miscellaneous JavaScript utilities",
  "license": "MIT",
  "main": "server.js",
  "files": [
    "superscript.js"
  ],
  "bugs": {
    "url": "https://github.com/tripu/superscript/issues",
    "email": "t@tripu.info"
  },
  "private": false,
  "preferGlobal": "false",
  "devDependencies": {
    "browserify": "16.2.3",
    "coveralls": "3.0.2",
    "eslint-plugin-node": "7.0.1",
    "eslint": "5.7.0",
    "istanbul": "0.4.5",
    "jsdoc": "3.5.5",
    "jshint": "2.9.6",
    "minami": "1.2.3",
    "mocha": "5.2.0"
  },
  "scripts": {
    "audit": "npm audit | grep -oP 'https://nodesecurity.io/advisories/(\\d+)' | diff known-vulns.txt -",
    "lint": "eslint server.js lib/ test/",
    "hint": "jshint server.js lib/ test/",
    "test": "mocha",
    "jsdoc": "jsdoc --configure jsdoc.json -r server.js lib/",
    "browserify": "browserify server.js -s superscript -o superscript.js",
    "build": "npm run audit && npm run lint && npm run hint && npm test && npm run jsdoc && npm run browserify",
    "coverage": "istanbul cover _mocha",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls"
  },
  "engines": {
    "node": "8 || 10",
    "npm": "6"
  }
}
